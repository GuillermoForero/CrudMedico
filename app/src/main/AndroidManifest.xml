<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.me.crudmedico">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.patient.view.FirmActivity"></activity>
        <activity android:name=".ui.patient.view.HistoryOfAppointmentsActivity" />
        <activity android:name=".ui.doctor.view.HistoryOfPatientsActivity" />
        <activity android:name=".ui.doctor.view.DoctorDetailActivity" />
        <activity android:name=".ui.patient.view.PatientDetailActivity" />
        <activity android:name=".ui.patient.view.CreatePatientActivity" />
        <activity android:name=".ui.doctor.view.CreateDoctorActivity" />
        <activity android:name=".ui.doctor.view.DoctorActivity" />
        <activity android:name=".ui.patient.view.PatientActivity" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>